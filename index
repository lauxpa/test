    <script>
        let currentSlide = 0;

        function nextSlide(increment) {
            document.getElementById('slide' + (currentSlide + 1)).style.display = 'none';
            currentSlide = Math.min(currentSlide + 1, 8);  // Limit to slide 8
            document.getElementById('slide' + (currentSlide + 1)).style.display = 'block';
        }
    </script>
